{
  "name": "smith-hub",
  "version": "1.0.1",
  "description": "Its about family",
  "engines": {
    "node": "12.x"
  },
  "main": "index.js",
  "scripts": {
    "start": "npm run server-prod",
    "dev": "concurrently \"npm run client-dev\"  \"npm run server-dev\"",
    "postinstall": "npm run client-prod",
    "server-dev": "cross-env NODE_ENV=development nodemon server/index.js",
    "server-prod": "cross-env NODE_ENV=production node server/index.js",
    "client-dev": "cross-env NODE_ENV=development webpack serve --config client/config/webpack.dev.js",
    "client-prod": "cross-env NODE_ENV=production webpack --config client/config/webpack.prod.js",
    "client-build-dev": "cross-env NODE_ENV=development webpack --config client/config/webpack.prod.js"
  },
  "author": "Sean Arseneault",
  "license": "ISC",
